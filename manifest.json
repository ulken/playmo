{
  "name": "Playmo",
  "version": "0.1.0",
  "description": "Gracefully add missing keyboard shortcuts to video players",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}

{
  "name": "Playmo",
  "version": "0.0.3",
  "description": "Add missing keyboard shortcuts to various streaming services",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": [
        "*://*.hbonordic.com/*",
        "*://*.dplay.se/*",
        "*://*.dplay.no/*",
        "*://*.dplay.dk/*"
      ]
    }
  ],
  "background": { "scripts": ["background.js"] }
}

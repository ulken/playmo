{
  "name": "Playmo",
  "version": "0.0.1",
  "description": "Add missing keyboard shortcuts to various streaming services",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": [
        "https://*.hbonordic.com/*/play",
        "https://*.dplay.se/*",
        "https://*.dplay.no/*",
        "https://*.dplay.dk/*"
      ]
    }
  ]
}
